<UserControl x:Class="Servidor2.ItemContexto"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:controles="clr-namespace:Inteldev.Core.Presentacion.Controles;assembly=Inteldev.Core.Presentacion"
             mc:Ignorable="d" Height="108.589" Width="508.562">
    <StackPanel>
        <controles:ItemFormulario
            Etiqueta="Nombre"
            Valor="{Binding Path=Objeto.Nombre, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
            ></controles:ItemFormulario>
        <controles:ItemFormulario
            Etiqueta="Contexto"
            Valor="{Binding Path=Objeto.StringConnecion, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
            ></controles:ItemFormulario>
    </StackPanel>
</UserControl>
